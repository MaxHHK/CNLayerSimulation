#include "TCPSource.hpp"



TCPPackage Source:: tcpToStr(char* str) {
TCPPackage msg;
strcpy(msg.flags, str);
strcpy(msg.sequence, str + 10);
strcpy(msg.ask, str + 20);
strcpy(msg.headLengthstr, str + 30);
strcpy(msg.checksum, str + 35);
return msg;
}

